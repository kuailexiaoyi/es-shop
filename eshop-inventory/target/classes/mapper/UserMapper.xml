<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.es.shop.inventory.mapper.UserMapper">

    <select id="queryAll" resultType="com.es.shop.inventory.entity.UserDO">
        select * from user
    </select>

    <select id="queryOne" resultType="com.es.shop.inventory.entity.UserDO">
        select * from user where id = #{id}
    </select>
</mapper>